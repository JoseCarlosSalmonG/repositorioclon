<%= simple_form_for(@reservationdetail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :quantityentree %>
    <%= f.input :quantitycourse %>
    <%= f.association :reservation %>
    <%= f.association :entree %>
    <%= f.association :course %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
